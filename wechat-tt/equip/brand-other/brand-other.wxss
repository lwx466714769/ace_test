/* equip/equip-type/equip-type.wxss */
.type-list{
  display: flex;
  padding: 80rpx;
  flex-wrap: wrap;
}
.type-item{
  width: 150rpx;
  height: 150rpx;
  margin-right: 70rpx;
  margin-bottom: 80rpx;
  position: relative;
  text-align: center;
}
.type-item:nth-child(3n){
  margin-right: 0;
}
.brand-img{
  width: 100%;
  height: 100%;
}
.equip-img-wrap{
  position: relative;
  background: #e6e6e6;
  width: 150rpx;
  height: 150rpx;
}
.equip-default{
  width: 84rpx;
  height: 64rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.type-name{
  margin-bottom: 10rpx;
}