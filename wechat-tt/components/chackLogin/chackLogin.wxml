<view class='member-wrap' catchtap='selectHIde' wx:if='{{!isShow}}'>
  <view class='member'>
    <view style='margin-left:30rpx;'>提示</view>
    <view class='line'></view>
    <view class='member-con'>
      审核未通过,{{deleteItems.refusereason === null ? "审核未通过请重新提交":deleteItems.refusereason}}
    </view>
    <view class='member-btns' style='background: #2a2a2a;'>
      <button class='btn' hover-class='btn-hover' catchtap='reSubmit'>重新提交</button>
      <view class='btnline'>|</view>
      <button catchtap='deleteCar' hover-class='btn-hover' class='btn'>删除</button>
    </view>
  </view>
</view>