<!--event/upload-event-result/upload-event-result.wxml-->
<view class='success'>
  <image class='icon-success' src='/images/icon-success.png'></image>
  <view class='success-font'>发布成功</view>
  <view class='success-font'>快去分享吧</view>
  <view class='btn-group'>
    <button class='btn btn-share' catchtap="showShare">分享</button>
    <navigator url='/event/event-detail/event-detail?activityId={{event.activityId}}' class='btn back'>去看看</navigator>
  </view> 
</view>
<Share id='share' shareparams='{{event}}' sharetype='event'/>
<canvas canvas-id="myCanvas" style="position:absolute;left:-200%;width:100%;height:420rpx;"></canvas>
