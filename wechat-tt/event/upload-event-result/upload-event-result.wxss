/* event/upload-event-result/upload-event-result.wxss */
page{
  width: 100%;
  height: 100%;
  background: #fff;
}
.success{
  width: 100%;
  height: 100%;
  background: #fff;
  text-align: center;
}
.icon-success{
  width: 100rpx;
  height: 100rpx;
  margin:0 auto 40rpx;
  display: block;
  padding-top: 200rpx;
}
.success-font{
  font-size: 36rpx;
  margin: 10rpx 0;
}
.info{
  font-size: 28rpx;
}
.btn-group{
  margin-top: 120rpx;
}
.btn{
  width: 292rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  margin: 0 auto 30rpx;
  border-radius: 8rpx;
  background: #000;
  color: #fff;
}
.back{
  background: #fff;
  color: #000;  
  border: solid 2rpx #000;
}