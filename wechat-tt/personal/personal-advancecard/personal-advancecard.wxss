.wrap {
  width: 100%;
  background: #fff;
}
.slot{
  height: 8rpx;
}
.btn-group{
  background: #000;
  display: inherit;
  margin-top: 10rpx;
}
.personal {
  text-align: right;
}

.my-btn {
  box-sizing: content-box;
  margin: unset;
  background: unset;
  padding: 0;
  font-size: 28rpx;
}

.wrap::after,.my-Mini::after{
  display: block;
  visibility: visible;
  height: 0;
  width: 0;
  clear: both;
  content: '';
}
.my-btn::after,.sign::after {
  display: none;
}

.personal {
  width: 750rpx;
  height: 194rpx;
  float: right;
  box-sizing: border-box;
}

.personal__wrapper {
  display: inline-flex;
}

.img {
  width: 64rpx;
  height: 64rpx;
}
.user-info{
  display: flex;
  align-items: center;
}
.avatar {
  display: block;
  width: 64rpx;
  height: 64rpx;
  box-sizing: border-box;
  border: 1rpx solid #fafafa;
  border-radius: 50%;
  overflow: hidden;
  float: right;
}

.login {
  display: flex;
  align-items: center;
  width: 100%;
  padding-left: 28rpx;
}

.info {
  display: flex;
  flex-direction: column;
  width: 100%;
  text-align: right;
  height: 194rpx;
}

.name {
  display: flex;
  text-align: right;
  flex-direction: row-reverse;
  padding-bottom: 12rpx;
  align-items: center;
  margin-top: 20rpx; 
}

.text {
  font-size: 28rpx;
  width: 580rpx;
  height: 40rpx;
  line-height: 60rpx;
  text-align: right;
}

.text text {
  display: block;
  text-align: right;
  width: 500rpx;
  padding-right: 20rpx;
}

.btn image{
  width: 32rpx;
  height: 32rpx;
  margin: 6rpx auto;

}

.sign{
  display: block;
  text-align: right;
  line-height: 40rpx;
  font-size: 28rpx;
  margin-right: 58rpx;
}
.nickname{
  width:214rpx;
  text-align: right;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.sign .signName{
  text-align: right;
  color: rgb(51, 51, 51);
  font-weight: bold;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.name-wrap{
  margin: 20rpx 0 0;
}


.newHand {
