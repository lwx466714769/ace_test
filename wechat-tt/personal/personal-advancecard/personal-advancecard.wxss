.cars-wrap{
  width: 750rpx;
  height: auto;
  background: white;
  overflow: hidden;
}
.cars-wrap::active{
  visibility: visible;
  width: 0;
  height: 0;
  display: block;
  clear: both;
  content: '';
}

.gift-picture {
  width: 750rpx;
  height: 360rpx;
}

.btnAndDescription {
  width: 750rpx;
  height: 80rpx;
  position: relative;
  background: rgba(0, 0, 0, 0.8);
  bottom: 0rpx;
  right: 0;
}

btnAndDescription::after {
  display: block;
  content: '';
  clear: both;
  visibility: visible;
  width: 0;
  height: 0;
}

.descriptionGift {
  width: 750rpx;
  height: 80rpx;
  overflow: hidden;
  line-height: 80rpx;
  text-overflow: ellipsis;
  display: block;
  color: white;
  background: rgba(0, 0, 0, 0.6);
  box-sizing: border-box;
  padding-left: 30rpx;
  font-size: 28rpx;
  font-weight: initial;
}

.descriptionGift, .getGift {
  float: left;
}

.getGift {
  width: 140rpx;
  height: 80rpx;
  color: rgb(236, 148, 4);
  font-size: 32rpx;
  border-radius: 0;
  display: block;
  line-height: 80rpx;
  padding: 0;
  background: rgba(0, 0, 0, 0.6);
}

