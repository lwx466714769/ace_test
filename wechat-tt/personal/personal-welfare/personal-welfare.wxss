.part_item_wrap {
  height: 182rpx;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  overflow: hidden;
  margin: 0 30rpx;
  display: flex;
  justify-content: space-between;
}
.order-info{
  flex: 1;
}
.order-info{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.order-name{
  font-size: 28rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  max-height: 80rpx;
  -webkit-line-clamp: 2;
  display: -webkit-box;
  -webkit-box-orient: vertical;
}
.total{
  display: flex;
  justify-content: space-between; 
}
.empty-wrap{
  margin-top: 400rpx;
}
.part_item{
  border-bottom: 3rpx solid #2a2a2a;
}

.card_cover {
  width: 182rpx;
  height: 182rpx;
  border-radius: 16rpx;
  margin-right: 30rpx;
}

.progress_num {
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;
  margin-bottom: 44rpx;
}

.allNumName, .overplusname {
  font-size: 24rpx;
  font-weight: lighter;
}

.allNum, .overplus {
  font-size: 24rpx;
}

.item_content_wrap {
  display: flex;
  justify-content: space-between;
  margin: 0 30rpx;
}

.item_content_name {
  display: flex;
  font-size: 28rpx;
}
.state{
  color: rgb(298,2,27);
}
.item_content_clubname{
  font-size: 32rpx;
  font-weight: bold;
}
.item_card_order{
  font-size: 24rpx;
  color: rgb(153,153,153);
}

