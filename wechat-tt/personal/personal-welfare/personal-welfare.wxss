.item_card_more {
  text-align: right;
  padding: 20rpx 0 ;
}

.more-icon {
  width: 16rpx;
  height: 14rpx;
  margin-bottom: 3rpx;
  margin-left: 14rpx;
}

.data_title{
  display: flex;
  justify-content: space-between;
  text-align: center;
  align-items: center;
  padding: 44rpx 0 24rpx;
}

.data_title view {
  width: 50%;
  font-weight: lighter;
}

view.bottom_line{
  width: 50rpx;
  height: 4rpx;
  border-radius: 4rpx;
  background-color: transparent;
  margin: 10rpx auto;
}

.backHome{
  width: 710rpx;
  background: white;
  position: fixed;
  right: 20rpx;
  bottom: 0;
}

.homeBtn{
  padding: 20rpx;
  text-align: center;
  color: white;
  font-weight: lighter;
  background: #2a2a2a;
  margin-bottom: 20rpx; 
}

.mainData{
  padding-bottom: 20rpx; 
