/**app.wxss**/
page{
  font-size: 28rpx;
  color: #333;
  font-family: PingFangSC, -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  position: relative;
  width: 100%;
  height: 100%;
}
page::after{
  visibility: visible;
  width: 0;
  height: 0;
  display: block;
  clear: both;
  content: '';
}

checkbox .wx-checkbox-input {
  width: 30rpx;
  height: 30rpx;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
button{
  border-radius: 0;
  font-size: 28rpx;
  background: none;
}
button:after{
  border:none;
}